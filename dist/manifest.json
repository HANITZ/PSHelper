{
  "manifest_version": 3,

  "name": "PSHelper",
  "description": "알고리즘 문제풀이 도우미",
  "version": "1.0",

  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "service_worker": "js/background.js"
  },

  "permissions": ["storage", "tabs"],

  "host_permissions": ["<all_urls>"]
}
